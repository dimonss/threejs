# Vite Base URL
# Base path for the application (used in vite.config.ts)
VITE_BASE_URL=/threejs/

# Development Server
VITE_DEV_PORT=5173
VITE_DEV_HOST=localhost

# Deployment Configuration
# Remote server host for deployment
DEPLOY_REMOTE_HOST=user@example.com

# Remote server path for deployment
DEPLOY_REMOTE_PATH=~/example/path/dist

# Feature Flags
# VITE_ENABLE_DEBUG=true
# VITE_ENABLE_STATS=false

# Three.js Configuration
# VITE_ENABLE_SHADOWS=true
# VITE_ENABLE_ANTIALIAS=true
